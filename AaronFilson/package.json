{
  "name": "tcp-serv",
  "version": "0.1.0",
  "description": "A simple tcp server / logger, which writes info to a log file.",
  "main": "index.js",
  "scripts": {
    "test": "mocha"
  },
  "keywords": [
    "tcp",
    "logger",
    "server"
  ],
  "author": "Aaron Filson",
  "license": "MIT",
  "devDependencies": {
    "chai": "^3.4.1",
    "gulp": "^3.9.0",
    "gulp-eslint": "^1.1.1",
    "gulp-mocha": "^2.2.0",
    "gulp-util": "^3.0.7",
    "mocha": "^2.3.4"
  },
  "eslintConfig": {
    "rules": {
      "indent": [
        2,
        2
      ],
      "quotes": [
        2,
        "single"
      ],
      "linebreak-style": [
        2,
        "unix"
      ],
      "semi": [
        2,
        "always"
      ]
    },
    "extends": "eslint:recommended",
    "env": {
      "es6": true,
      "node": true,
      "mocha": true
    }
